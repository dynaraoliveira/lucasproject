<p id="notice"><%= notice %></p>

<h1>Api V1 Sensors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_sensors.each do |api_v1_sensor| %>
      <tr>
        <td><%= link_to 'Show', api_v1_sensor %></td>
        <td><%= link_to 'Edit', edit_api_v1_sensor_path(api_v1_sensor) %></td>
        <td><%= link_to 'Destroy', api_v1_sensor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Sensor', new_api_v1_sensor_path %>
